{
  "standalone": false,
  "initialActivePage": "blocks",
  "initialActiveCard": "favorites",
  "debug": "silly",
  "ignoredProcessNames": ["Rainmeter", "NVIDIA Share"],
  "globalStyleVariables": {
    "viewport-height": "100svh"
  },
  "toolbar": [
    {
      "name": "upload",
      "type": "upload",
      "icon": "iconoir-upload"
    },
    {
      "name": "search",
      "type": "search",
      "icon": "iconoir-search"
    },
    {
      "name": "status",
      "type": "status"
    }
  ],
  "pages": [
    {
      "name": "blocks",
      "icon": "iconoir-rhombus",
      "text": "Blocks",
      "cards": [
        {
          "name": "favorites",
          "title": "Favorites",
          "designVertical": [
            "volume-down volume-up",
            "previous next",
            "discord-mute play"
          ],
          "designHorizontal": [
            "volume-down volume-up previous next",
            ". . play discord-mute"
          ],
          "blocks": [
            {
              "name": "volume-down",
              "command": "press down",
              "repeatInterval": 20,
              "icon": "iconoir-sound-low",
              "text": "Lower"
            },
            {
              "name": "volume-up",
              "command": "press up",
              "repeatInterval": 20,
              "icon": "iconoir-sound-high",
              "text": "Higher"
            },
            {
              "name": "discord-mute",
              "command": "press alt+x",
              "icon": "iconoir-mic-mute",
              "text": "Mute",
              "background": "#95BDFF",
              "color": "#000"
            },
            {
              "name": "previous",
              "command": "press left",
              "icon": "iconoir-rewind",
              "text": "Rewind"
            },
            {
              "name": "next",
              "command": "press right",
              "icon": "iconoir-forward",
              "text": "Forward"
            },
            {
              "name": "play",
              "command": "press spc",
              "icon": "iconoir-play",
              "text": "Play/Pause"
            }
          ]
        },
        {
          "name": "vlc",
          "title": "VLC",
          "design": [
            "volume-down volume-up",
            "rewind forward",
            "prev-chapter next-chapter",
            "prev-video next-video",
            "cycle-subtitle cycle-audio",
            "fullscreen play "
          ],
          "blocks": [
            {
              "name": "volume-down",
              "command": "press down",
              "repeatInterval": 20,
              "icon": "iconoir-sound-low",
              "text": "Lower"
            },
            {
              "name": "volume-up",
              "command": "press up",
              "repeatInterval": 20,
              "icon": "iconoir-sound-high",
              "text": "Higher"
            },
            {
              "name": "rewind",
              "command": "press left",
              "icon": "iconoir-rewind",
              "text": "Rewind"
            },
            {
              "name": "forward",
              "command": "press right",
              "icon": "iconoir-forward",
              "text": "Forward"
            },
            {
              "name": "prev-chapter",
