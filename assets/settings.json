              "icon": "iconoir-arrow-left",
              "text": "left"
            },
            {
              "name": "down",
              "command": "press down",
              "icon": "iconoir-arrow-down",
              "text": "down"
            },
            {
              "name": "right",
              "command": "press right",
              "icon": "iconoir-arrow-right",
              "text": "right"
            }
          ]
        }
